version: "3"
services:
  prometheus:
    image: prom/prometheus
    ports:
      - "9090:9090"
    volumes:
      - "./prometheus.yml:/etc/prometheus/prometheus.yml"
  app:
    build: .
    command: myproject
    ports:
      - "9592:9592"
    volumes:
      - "./credentials.json:/app/credentials.json"